{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/mouva-vet/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/mouva-vet/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/mouva-vet",
  "projects": {
    "billing-service": {
      "type": "application",
      "root": "apps/billing-service",
      "entryFile": "main",
      "sourceRoot": "apps/billing-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/billing-service/tsconfig.app.json"
      }
    },
    "care-service": {
      "type": "application",
      "root": "apps/care-service",
      "entryFile": "main",
      "sourceRoot": "apps/care-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/care-service/tsconfig.app.json"
      }
    },
    "hr-service": {
      "type": "application",
      "root": "apps/hr-service",
      "entryFile": "main",
      "sourceRoot": "apps/hr-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/hr-service/tsconfig.app.json"
      }
    },
    "inventory-service": {
      "type": "application",
      "root": "apps/inventory-service",
      "entryFile": "main",
      "sourceRoot": "apps/inventory-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/inventory-service/tsconfig.app.json"
      }
    },
    "login-service": {
      "type": "application",
      "root": "apps/login-service",
      "entryFile": "main",
      "sourceRoot": "apps/login-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/login-service/tsconfig.app.json"
      }
    },
    "medical-record-service": {
      "type": "application",
      "root": "apps/medical-record-service",
      "entryFile": "main",
      "sourceRoot": "apps/medical-record-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/medical-record-service/tsconfig.app.json"
      }
    },
    "mouva-vet": {
      "type": "application",
      "root": "apps/mouva-vet",
      "entryFile": "main",
      "sourceRoot": "apps/mouva-vet/src",
      "compilerOptions": {
        "tsConfigPath": "apps/mouva-vet/tsconfig.app.json"
      }
    },
    "patient-service": {
      "type": "application",
      "root": "apps/patient-service",
      "entryFile": "main",
      "sourceRoot": "apps/patient-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/patient-service/tsconfig.app.json"
      }
    }
  }
}